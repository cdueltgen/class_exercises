def add(num1, num2):
    return num1 + num2

def subtract(num1, num2):
    return num1 - num2

def multiply(num1, num2):
    return num1 * num2

def divide(num1, num2):
    return float(num1) / num2

def square(num1):
    return num1 * num1

def cube(num1):
    return num1 * num1 * num1

def power(num1, num2):
    result = 1
    for i in range(num2):
        result = result * num1
        #result = 1*num1 = num1
        #result = num1 * num1 = num1^2
        #result =
        return result

def mod(num1, num2):
    return num1 % num2